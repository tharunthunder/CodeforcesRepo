import java.util.*;
public class Stair_Peak_or_Neither {
public static void main(String[]args)
{
	Scanner sc = new Scanner(System.in);
	int T = sc.nextInt();
	for(int i = 0;i<T;i++)
	{
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		if(a<b&&b<c)
		{
			System.out.println("STAIR");
		}
		else if(a<b&&b>c)
		{
			System.out.println("PEAK");
		}
		else
		{
			System.out.println("NONE");
		}
	}
}
}
